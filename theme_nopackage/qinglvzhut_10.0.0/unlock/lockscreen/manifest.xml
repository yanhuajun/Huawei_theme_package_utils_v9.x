    <Var name="oXn" expression="0" const="true" />	
	<Var name="oXp" expression="0" const="true" />	
	<Var name="oXq" expression="min((#time-#oXp)*(10/4),1000)/1000" />	
	<Var name="oXl" expression="-(#oXq-1)*(#oXq-1)*(#oXq-1)" />	
	<Var name="move_y" expression="0" const="true"/>	
	<Var name="move_y" expression="ifelse(#setting,min(#touch_y-#touch_begin_y,0),#oXn*#oXl)" />
	<Var name="ul_m" expression="ifelse(eq(#moving_target,0),#move_y,0)" />
	<Var name="moving_target" expression="0" const="true" /> 
	<Var name="oXr" expression="1" const="true" type="number" />	
	<Var name="oXf" expression="0" const="true" />	
	<Var name="oXg" expression="0" const="true" />	
	<Var name="oXh" expression="min((#time-#oXf)*(10/4),1000)/1000" />	
	<Var name="oXi" expression="ifelse(#oXg,(#oXh-1)*(#oXh-1)*(#oXh-1)+1,-(#oXh-1)*(#oXh-1)*(#oXh-1))" />
	<Var name="oXj" expression="500*#oXi" />
	<Var name="sw" expression="#screen_width" />
	<Var name="sh" expression="#screen_height" />
	<Var name="setting" expression="0" const="true" />
	<Var name="set1" expression="1" const="true"  persist="true" />
	<Var name="yuju" expression="1" const="true"  persist="true" />
	<Var name="wz" expression="1" const="true" type="number" />	
	<Var name="oyn" expression="0" const="true" />	
	<Var name="oym" expression="0" const="true" />	
	<Var name="oyo" expression="0" const="true" />	
	<Var name="oyp" expression="min((#time-#oyo)*(10/4),1000)/1000" />	
	<Var name="oyk" expression="-(#oyp-1)*(#oyp-1)*(#oyp-1)" />	
	<Var name="oyj" expression="ifelse(#oyn,max(#touch_begin_x-#touch_x,0),#oym*#oyk)" />		
	<Var name="oyq" expression="1" const="true" type="number" />	
	<Var name="oye" expression="0" const="true" />	
	<Var name="oyf" expression="0" const="true" />	
	<Var name="oyg" expression="min((#time-#oye)*(10/4),1000)/1000" />	
	<Var name="oyh" expression="ifelse(#oyf,(#oyg-1)*(#oyg-1)*(#oyg-1)+1,-(#oyg-1)*(#oyg-1)*(#oyg-1))" />	
	<Var name="oyi" expression="500*#oyh" />	
	<Var name="oyb" expression="ifelse(#oyd,8*#oyc*#oyc*#oyc,1-8*#oyc*#oyc*#oyc)" />		 

	<Var name="kyn" expression="0" const="true" />	
	<Var name="kym" expression="0" const="true" />	
	<Var name="kyo" expression="0" const="true" />	
	<Var name="kyp" expression="min((#time-#kyo)*(10/4),1000)/1000" />	
	<Var name="kyk" expression="-(#kyp-1)*(#kyp-1)*(#kyp-1)" />	
	<Var name="kyj" expression="ifelse(#kyn,max(#touch_begin_x-#touch_x,0),#kym*#kyk)" />		
	<Var name="kyq" expression="1" const="true" type="number" />	
	<Var name="kye" expression="0" const="true" />	
	<Var name="kyf" expression="0" const="true" />	
	<Var name="kyg" expression="min((#time-#kye)*(10/4),1000)/1000" />	
	<Var name="kyh" expression="ifelse(#kyf,(#kyg-1)*(#kyg-1)*(#kyg-1)+1,-(#kyg-1)*(#kyg-1)*(#kyg-1))" />	
	<Var name="kyi" expression="500*#kyh" />	
	<Var name="kyb" expression="ifelse(#oyd,8*#oyc*#oyc*#oyc,1-8*#oyc*#oyc*#oyc)" />



